<p id="notice"><%= notice %></p>
<% provide(:title,"bootstrap")%> 

<div class="center jumbotron">
<h1>Alpha Phi</h1>
</div>

<h2>User Information</h2>

<table style=" background-repeat:no-repeat; width:450px;margin:0;" cellpadding="0" cellspacing="0" border="10">
 <tr style="height:40px; width:40px; margin:0; color:black; background-color: white;" >
      <th>Name</th>
      <th>Restriction</th>
      <th>Email</th>
      <th>Eligibility</th>
 </tr>
<% if @accounts %>
<% for e in @accounts do %>
<tr>
    <td><%= e.name %></td>
    <td><%= e.restriction %></td>
    <td><%= e.email %></td>
    <td><%= e.eligibility %></td>
    <% end %>
    <% end %>
</tr>
</table>

