<% provide(:title, "bootstrap")%>
<div class="center jumbotron">
<h1>Users</h1>
</div>

<div class="col-md-4 col-md-offset-3">
<table class="table table-striped">
    
    <tr>
        <th>User Name</th>
        <th>Email</th>
    </tr>
    <tbody>
        <% @users = User.all %>
        <% @users.each do |user| %>
        <tr>
        <td><%= user.name %></td>
        <td><%= user.email%></td>
        </tr>
        <% end %>
    </tbody>
    
</table>
</div>

<br>
<br>